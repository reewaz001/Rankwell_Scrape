# Browser Configuration
# Set to false to see the browser window (useful for debugging)
BROWSER_HEADLESS=false

# Lightpanda Local Browser Configuration
# These are optional - defaults will be used if not specified
LIGHTPANDA_HOST=127.0.0.1
LIGHTPANDA_PORT=9222

# Scraping Configuration
REQUEST_DELAY=300
PAGE_LIMIT=100

# Database/Backend API Configuration
BACKEND_API_URL=https://rankwell.one/api
BACKEND_API_TIMEOUT=30000

# Dashboard API Configuration
DASHBOARD_BASE_URL=https://rankwell.one/api

# Logging
LOG_LEVEL=info
LOG_FILE=logs/scraper.log

# BQS Scoring Configuration
BQS_T_MAX=100000
BQS_RD_MAX=1000
BQS_HARD_FILTER=true

# Google Search Console API Configuration
# See GOOGLE_SEARCH_CONSOLE_SETUP.md for detailed setup instructions

# Option 1: Service Account (Recommended for server-to-server)
# Path to service account JSON key file
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=./credentials/google-service-account.json

# Option 2: OAuth2 (For user authorization)
# Uncomment and configure if using OAuth2 instead of Service Account
# GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
# GOOGLE_CLIENT_SECRET=your-client-secret
# GOOGLE_REDIRECT_URI=http://localhost:3000/oauth2callback
# GOOGLE_REFRESH_TOKEN=your-refresh-token


# Server Configuration
PORT=3000
NODE_ENV=development


# Cron Job Configuration
# Set to 'true' to enable automated monthly scraper jobs
# When enabled, the following tasks run on the last day of each month at 9:00 PM:
#   1. test:netlink-scraper
#   2. test:netlink-scraper-rest (runs after step 1 completes)
ENABLE_CRON=false
# Timezone for cron job scheduling (e.g., 'Europe/Paris', 'America/New_York', 'UTC')
CRON_TIMEZONE=Europe/Paris
